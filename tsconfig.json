{
  "compilerOptions": {
    "target": "es6",
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}


